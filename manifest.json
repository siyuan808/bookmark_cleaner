{
  "manifest_version": 2,

  "name": "Bookmark Cleaner",
  "description": "Make your bookmarks management much more easier!",
  "version": "1.0",
  
  "permissions": [
  	"bookmarks",
    "history",
    "storage",
    "chrome://favicon/",
    "tabs"
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "helper.html"
  }
}